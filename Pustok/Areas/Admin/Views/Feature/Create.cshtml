@model Feature
<div class="container">
    <h1>Create Feature</h1>
    <form method="post">
        <div class="mb-3">
            <label asp-for="Title" class="form-label">Title</label>
            <input asp-for="Title" class="form-control" placeholder="Title">
            <span asp-validation-for="Title" style="color:red;"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Description" class="form-label">Description</label>
            <input asp-for="Description" class="form-control" placeholder="Description">
            <span asp-validation-for="Description" style="color:red;"></span>

        </div>
        <div class="mb-3">
            <label asp-for="IconUrl" class="form-label">IconUrl</label>
            <input asp-for="IconUrl" class="form-control" placeholder="IconUrl">
            <span asp-validation-for="IconUrl" style="color:red;"></span>

        </div>

        


        <button type="submit" class="btn btn-primary">Create</button>
    </form>

</div>
